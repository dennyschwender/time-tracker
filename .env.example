# TimeTracking Web App Environment Variables
# Copy this file to .env and set your own values

# Flask secret key for session management (generate a random string)
SECRET_KEY=change-this-to-a-random-secret-key-in-production

# Enable server-side database persistence (1 = enabled, 0 = disabled)
USE_SERVER_DB=1

# Server port (optional, defaults to 5000)
# PORT=5000
